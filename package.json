{
  "author": "Ephigenia M. Eichner",
  "name": "mite-cli",
  "version": "1.2.1",
  "description": "command line tool for time tracking service mite.yo.lk",
  "preferGlobal": true,
  "homepage": "https://github.com/Ephigenia/mite-cli/",
  "keywords": [
    "budgets",
    "cli",
    "command-line-tool",
    "commandline",
    "list",
    "mite",
    "project",
    "shell",
    "time-tracking",
    "tool"
  ],
  "license": "MIT",
  "maintainer": {
    "name": "Marcel Eichner",
    "email": "marcel.eichner@ephigenia.de"
  },
  "contributors": [
    {
      "name": "Marcel Eichner",
      "email": "marcel.eichner@ephigenia.de"
    }
  ],
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ephigenia/mite-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Ephigenia/mite-cli/issues"
  },
  "bin": {
    "mite": "source/mite.js"
  },
  "npmCheck": {
    "ignore": "{lodash.template,lodash,mem}"
  },
  "main": "source/mite.js",
  "scripts": {
    "changelog:preview": "conventional-changelog --output-unreleased -p angular",
    "changelog": "conventional-changelog -s -i CHANGELOG.md -p angular -r 0",
    "lint": "eslint source",
    "lint:fix": "npm run lint -- --fix",
    "postversion": "git push && git push --tags",
    "preversion": "npm test && npm run lint && npm run test:outdated",
    "start": "node $npm_package_main",
    "tdd": "npm run test -- --watch",
    "test": "mocha --check-leaks source/**/*test.js --exit",
    "test:outdated": "npm outdated",
    "test:security": "snyk test --severity-threshold=high",
    "update": "npx npm-check --update",
    "version:recommend": "conventional-recommended-bump --preset angular",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^4.0.0",
    "csv-string": "^3.1.6",
    "external-editor": "^3.1.0",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "lodash.template": ">=4.5.0",
    "markdown-table": "^1.1.3",
    "mite-api": "^0.1.1",
    "nconf": "^0.10.0",
    "node-fetch": "^2.6.0",
    "opn": "^6.0.0",
    "table": "^5.4.6",
    "tabtab": "^3.0.2",
    "weeknumber": "^1.1.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.25",
    "conventional-recommended-bump": "^6.0.2",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2"
  },
  "optionalDependencies": {
    "npm-check": "^5.9.0"
  }
}
