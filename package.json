{
  "author": "Ephigenia M. Eichner",
  "name": "mite-cli",
  "version": "1.4.0",
  "description": "command line tool for time tracking service mite.yo.lk",
  "preferGlobal": true,
  "homepage": "https://github.com/Ephigenia/mite-cli/",
  "keywords": [
    "budgets",
    "cli",
    "command-line-tool",
    "commandline",
    "list",
    "mite",
    "project",
    "shell",
    "time-tracking",
    "tool"
  ],
  "license": "MIT",
  "maintainer": {
    "name": "Marcel Eichner",
    "email": "marcel.eichner@ephigenia.de"
  },
  "contributors": [
    {
      "name": "Marcel Eichner",
      "email": "marcel.eichner@ephigenia.de"
    }
  ],
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ephigenia/mite-cli.git"
  },
  "funding": "http://github.com/sponsors/Ephigenia",
  "bugs": {
    "url": "https://github.com/Ephigenia/mite-cli/issues"
  },
  "bin": {
    "mite": "source/mite.js"
  },
  "main": "source/mite.js",
  "scripts": {
    "audit": "npm audit",
    "audit:strict": "npm run -s audit -- --only=prod --audit-level=high",
    "changelog:preview": "conventional-changelog --output-unreleased -p angular",
    "changelog": "conventional-changelog -s -i CHANGELOG.md -p angular -r 0",
    "lint": "eslint source",
    "lint:fix": "npm run lint -- --fix",
    "postversion": "git push && git push --tags",
    "preversion": "npm test && npm run lint && npm run audit:strict",
    "start": "node $npm_package_main",
    "tdd": "npm run test -- --watch",
    "test": "mocha --check-leaks --exit source/**/*test.js",
    "test:security": "snyk test --severity-threshold=high",
    "version:recommend": "conventional-recommended-bump --preset angular",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^4.1.1",
    "csv-string": "^4.0.1",
    "external-editor": "^3.1.0",
    "inquirer": "^7.1.0",
    "markdown-table": "^2.0.0",
    "mite-api": "^0.1.1",
    "nconf": "^0.10.0",
    "node-fetch": "^2.6.0",
    "opn": "^6.0.0",
    "table": "^5.4.6",
    "tabtab": "^3.0.2",
    "weeknumber": "^1.1.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.0.34",
    "conventional-recommended-bump": "6.0.9",
    "eslint": "7.3.1",
    "mocha": "7.2.0"
  }
}
